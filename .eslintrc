{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-typescript",
    "plugin:@typescript-eslint/eslint-recommended"
  ],
  "rules": {
    "semi": "error",
    "no-console": "off",
    "no-async-promise-executor": "off"
  }
}